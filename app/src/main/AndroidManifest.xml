<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.peterzhang.healthhui" >
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".heathmsg.HealthMesseageActivity"
            android:label="@string/title_activity_health_messeage"
            >
        </activity>

        <activity android:name=".heathmsg.HealthMsgCategoryDetailActivity"
            android:label="@string/title_activity_health_category_detail"
            >
        </activity>

        <activity android:name=".heathmsg.HealthMsgItemDetailActivity"
            android:label="@string/title_activity_health_msg_item_detail"
            >
        </activity>


        <meta-data
            android:name="channel"
            android:value="${channelName}" />

    </application>

</manifest>

